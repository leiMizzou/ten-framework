{
  "type": "extension",
  "name": "aliyun_tts_realtime_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.4.0"
    },
    {
      "type": "system",
      "name": "ten_ai_base",
      "version": "0.10.0"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "**.py",
      "requirements.txt"
    ]
  },
  "api": {
    "interface": [
      {
        "import_uri": "../../system/ten_ai_base/api/tts-interface.json"
      }
    ],
    "property": {
      "properties": {
        "api_key": {
          "type": "string"
        },
        "model": {
          "type": "string"
        },
        "ws_url": {
          "type": "string"
        },
        "voice": {
          "type": "string"
        },
        "format": {
          "type": "string"
        },
        "sample_rate": {
          "type": "int32"
        },
        "mode": {
          "type": "string"
        }
      }
    }
  }
}
